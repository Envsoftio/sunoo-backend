{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowS3UploadAndManagement",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::sunoo-app",
        "arn:aws:s3:::sunoo-app/*"
      ]
    }
  ]
}

